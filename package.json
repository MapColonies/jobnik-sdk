{
  "name": "@map-colonies/jobnik-sdk",
  "version": "1.0.0",
  "description": "Map Colonies Jobnik SDK",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && copyfiles -f src/openapi3.yaml dist",
    "start": "npm run build && cd dist && node ./index.js",
    "clean": "rimraf dist",
    "generate:openapi": "node scripts/openapi.mts",
    "prepack": "npm run build",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/ts-npm-package-boilerplate.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/ts-npm-package-boilerplate/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/MapColonies/ts-npm-package-boilerplate#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@map-colonies/commitlint-config": "^1.1.1",
    "@map-colonies/eslint-config": "^6.0.0",
    "@map-colonies/openapi-helpers": "^4.0.0",
    "@map-colonies/prettier-config": "^0.0.1",
    "@map-colonies/tsconfig": "^1.0.1",
    "@opentelemetry/context-async-hooks": "^2.0.1",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@types/node": "^22.13.11",
    "@types/opossum": "^8.1.9",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "commitlint": "^19.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "jest-extended": "^6.0.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.12",
    "typescript": "^5.8.2",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@map-colonies/read-pkg": "^1.0.0",
    "@readme/openapi-parser": "^4.0.1",
    "date-fns": "^4.1.0",
    "http-status-codes": "^2.3.0",
    "openapi-fetch": "^0.14.0",
    "opossum": "^9.0.0",
    "undici": "^6.21.3",
    "yaml": "^2.8.1"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0"
  }
}
